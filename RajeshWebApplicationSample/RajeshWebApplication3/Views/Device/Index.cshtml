@model IEnumerable<RajeshWebApplication3.Models.DeviceData>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "CreateNew")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Model)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SerialNum)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Type)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Model)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SerialNum)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Type)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { Name = item.Name, Model = item.Model, SerialNum = item.SerialNum, Type = item.Type }) |
            @Html.ActionLink("Details", "Details", new { Name=item.Name,Model=item.Model,SerialNum=item.SerialNum,Type=item.Type }) |
            @Html.ActionLink("Delete", "Delete", new { Name = item.Name, Model = item.Model, SerialNum = item.SerialNum, Type = item.Type })
        </td>
    </tr>
}

</table>
